<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black_ecec">

    <!--<LinearLayout-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"-->
        <!--android:orientation="vertical"-->
        <!--android:paddingBottom="25dp"-->
        <!--android:background="@color/black_ecec">-->

        <RelativeLayout
            android:id="@+id/relative_about"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="10dp"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:drawablePadding="10dp"
                android:gravity="center"
                android:drawableLeft="@drawable/icon_huaxia"
                android:text="关于花虾"
                android:textSize="14sp"
                android:textColor="@color/black"
                android:background="@color/transparent"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:src="@drawable/icon_next"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/relative_question"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="10dp"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:drawablePadding="10dp"
                android:gravity="center"
                android:drawableLeft="@drawable/icon_question_j"
                android:text="常见问题"
                android:textSize="14sp"
                android:textColor="@color/black"
                android:background="@color/transparent"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:src="@drawable/icon_next"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@drawable/line"/>
        <RelativeLayout
            android:id="@+id/relative_exemption"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="0dp"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:drawablePadding="10dp"
                android:gravity="center"
                android:drawableLeft="@drawable/icon_exemption"
                android:text="免责声明"
                android:textSize="14sp"
                android:textColor="@color/black"
                android:background="@color/transparent"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:src="@drawable/icon_next"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/relative_privacy"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="10dp"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:drawablePadding="10dp"
                android:gravity="center"
                android:drawableLeft="@drawable/icon_privacy"
                android:text="隐私政策"
                android:textSize="14sp"
                android:textColor="@color/black"
                android:background="@color/transparent"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:src="@drawable/icon_next"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@drawable/line"/>
        <RelativeLayout
            android:id="@+id/relative_notice_more"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="0dp"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:drawablePadding="10dp"
                android:gravity="center"
                android:drawableLeft="@drawable/icon_notice_n"
                android:text="花虾公告"
                android:textSize="14sp"
                android:textColor="@color/black"
                android:background="@color/transparent"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:src="@drawable/icon_next"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relative_notice_update"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="10dp"
            android:background="@color/white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:drawablePadding="10dp"
                android:gravity="center"
                android:drawableLeft="@drawable/icon_updat"
                android:text="检查更新"
                android:textSize="14sp"
                android:textColor="@color/black"
                android:background="@color/transparent"
                />

            <TextView
                android:id="@+id/tv_version_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:text="当前版本V2.0"
                android:textSize="14sp"
                android:textColor="@color/black_9999"
                android:drawablePadding="5dp"
                android:drawableRight="@drawable/icon_next"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relative_phone"
            android:layout_width="match_parent"
            android:layout_height="68dp"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="28dp"
            android:layout_marginRight="28dp"
            android:background="@drawable/bg_more_phone">

            <ImageView
                android:id="@+id/img_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="28dp"
                android:src="@drawable/icon_phone_m"/>

            <TextView
                android:id="@+id/tv_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/img_phone"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="58dp"
                android:text="@string/text_phone"
                android:textSize="20sp"
                android:textColor="@color/orange_ffb9"
                android:background="@color/transparent"
                />
            <TextView
                android:id="@+id/tv_phone_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_phone"
                android:layout_toRightOf="@+id/img_phone"
                android:layout_marginLeft="58dp"
                android:layout_marginTop="0dp"
                android:text="客服热线"
                android:textSize="12sp"
                android:textColor="@color/black_3333"
                android:background="@color/transparent"
                />
        </RelativeLayout>
        <TextView
            style="@style/textWarmingInvest"
            android:layout_height="wrap_content"
            />
    </LinearLayout>

<!--</ScrollView>-->